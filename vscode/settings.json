{
    "editor.tabSize": 2,
    "workbench.iconTheme": "material-icon-theme",
    "editor.minimap.enabled": false,
    "git.enabled": false,
    "merge-conflict.codeLens.enabled": false,
    "rust.mode": "legacy",
    "editor.insertSpaces": false,
    "[dockerfile]": {
        "editor.parameterHints": true,
        "editor.quickSuggestions": {
            "other": true,
            "comments": false,
            "strings": false
        }
    },
    "material-icon-theme.files.associations": {
        "docker-compose.task.yml": "docker",
        "docker-compose.dev.yml": "docker",
        "docker-compose.prod.yml": "docker",
        "docker-compose.swarm.yml": "docker",
        "docker-stack.yml": "docker",
        "Dockerfile.dev": "docker",
        "Dockerfile.prod": "docker"
    },
    "mssql.connections": [
        {
            "server": "localhost",
            "database": "",
            "authenticationType": "SqlLogin",
            "user": "sa",
            "password": "",
            "emptyPasswordInput": false,
            "savePassword": true,
            "profileName": "local"
        },
        {
            "server": "localhost,1433",
            "database": "",
            "authenticationType": "SqlLogin",
            "user": "sa",
            "password": "",
            "emptyPasswordInput": false,
            "savePassword": true,
            "profileName": "container"
        },
        {
            "server": "localhost,1433",
            "database": "",
            "authenticationType": "SqlLogin",
            "user": "sa",
            "password": "",
            "emptyPasswordInput": false,
            "savePassword": true,
            "profileName": "container demo"
        }
    ],
    "material-icon-theme.activeIconPack": "angular_ngrx",
    "spellright.ignoreFiles": [
        // "**/!*.md"
    ],
    "markdown.extension.toc.githubCompatibility": true,
    "editor.renderWhitespace": "all",
    "editor.minimap.maxColumn": 80,
    "editor.wordWrapColumn": 120,
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "java.errors.incompleteClasspath.severity": "ignore",
    "java.configuration.checkProjectSettingsExclusions": false,
    "emmet.triggerExpansionOnTab": true,
    "emmet.includeLanguages": {
        "aspnetcorerazor": "html"
    },
    "emmet.optimizeStylesheetParsing": false,
    "emmet.showExpandedAbbreviation": "always",
    "editor.formatOnType": true,
    "editor.fontSize": 16,
    "[python]": {},
    "files.associations": {
        "*.py": "python"
    },
    "C_Cpp.workspaceSymbols": "All",
    "C_Cpp.clang_format_fallbackStyle": "",
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "liveServer.settings.port": 3000,
    "go.useLanguageServer": true,
    "security.workspace.trust.untrustedFiles": "open",
    "workbench.colorTheme": "Darkula"
}